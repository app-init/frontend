{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env",
          { "modules": false }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import"
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime"
      ]
    }
  }
}
